<?xml version="1.0" encoding="utf-8"?>
<!--suppress ALL -->
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="atafies.degreehacks">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="atafies.degreehacks.MainActivity"
            android:configChanges="keyboardHidden|orientation|screenSize">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="atafies.degreehacks.BaseDrawerActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:label="@string/title_activity_base_drawer"
            android:parentActivityName="atafies.degreehacks.MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="atafies.degreehacks.MainActivity" />
        </activity>
        <activity android:name="atafies.degreehacks.GPACalculatorActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"></activity>
        <activity android:name="atafies.degreehacks.ClassroomActivity"
            android:configChanges="keyboardHidden|orientation|screenSize">

        </activity>
        <activity android:name="atafies.degreehacks.GradeTrackerActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"></activity>
        <activity android:name="atafies.degreehacks.NotesActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"></activity>
        <!-- ATTENTION: This was auto-generated to add Google Play services to your project for
             App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
    </application>

</manifest>
